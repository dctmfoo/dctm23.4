{"rules":[{"id":"1696","name":"Sample OpenText Directory Services (OTDS) 16.0.0 and above","whitelist":["(?i)^(otdsws|otds-v2|ot-authws|otds-admin)(/.*|$)"],"blacklist":[{"match":"(?i)^otdsws/rest/(accessroles|authhandlers|registeredauthhandlers|connectors|consolidation|currentusers|groups|identityproviderprofiles|identityproviders|jobs|orgunits|partitions|resources|systemconfig|users|usersgroups)/","responseCode":404,"location":null},{"match":"(?i)^otdsws/rest/?$","responseCode":404,"location":null},{"match":"(\\.\\./)|(//)","responseCode":400,"location":null},{"match":"(?i)^otdsws/api","responseCode":404,"location":null}],"urlRules":[{"match":"(?i)^otdsws/?(.*|$)","replace":"http://otds.myhost.com:8080/otdsws/$1","sort":0,"continue":false},{"match":"(?i)^otds-v2/?(.*|$)","replace":"http://otds.myhost.com:8080/otds-v2/$1","sort":1,"continue":false},{"match":"(?i)^ot-authws/?(.*|$)","replace":"http://otds.myhost.com:8080/ot-authws/$1","sort":2,"continue":false},{"match":"(?i)^otds-admin/?(.*|$)","replace":"http://otds.myhost.com:8080/otds-admin/$1","sort":3,"continue":false}],"outgoingRules":[{"urlMatch":".*","match":"baseUrl\\s*:\\s*'(js)'","replace":"baseUrl:'otds-admin/$1'","scope":"RESPONSE_BODY","sort":0,"continue":true},{"urlMatch":".*","match":"<\\s*script\\s+src\\s*=\\s*(\"|')js/","replace":"<script src=$1otds-admin/js/","scope":"RESPONSE_BODY","sort":1,"continue":true},{"urlMatch":".*","match":"<\\s*link\\s*href\\s*=\\s*(\"|')css","replace":"<link href=$1otds-admin/css","scope":"RESPONSE_BODY","sort":2,"continue":true},{"urlMatch":".*","match":"<\\s*img\\s+(.*)src=('|\")img/","replace":"<img $1 src=$2otds-admin/img/","scope":"RESPONSE_BODY","sort":3,"continue":true},{"urlMatch":".*","match":"this.options.currentUser.serviceName\\s*=\\s*\"otdsws\"","replace":"this.options.currentUser.serviceName = \"otdsws\"","scope":"RESPONSE_BODY","sort":4,"continue":true}],"enabled":false,"sort":13,"enableOAuth":true}]}